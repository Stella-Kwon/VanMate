{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "db:migrate": "npx mikro-orm migration:generate",
    "db:update": "npx mikro-orm migration:up",
    "db:rollback": "npx mikro-orm migration:down",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "tsx -r tsconfig-paths/register src/server.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/cookie": "^9.2.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^10.0.0",
    "@mikro-orm/core": "^6.5.9",
    "@mikro-orm/postgresql": "^6.5.9",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.29.1",
    "google-auth-library": "^10.5.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "redis": "^5.9.0",
    "reflect-metadata": "^0.2.2",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^13.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.24",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3"
  }
}
